{
    "env": {
        "browser": true,
        "commonjs": true,
        "jquery": true
    },
    "extends": "airbnb-base/legacy",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": 0
    }
}